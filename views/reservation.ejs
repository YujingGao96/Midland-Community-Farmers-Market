<!-- Reservation Section -->
<section class="page-section bg-dark text-white" id="reservation">
    <div class="container text-center">
        <h1 class="mb-4">Reservation</h1>
        <hr class="divider my-4">

        <% if (specialEvent.specialEventSwitch !== 'ON' && weeklyMarket.weeklyMarketSwitch !== 'ON') { %>
            <%- include('no-registration', {}); %>
        <% } %>

        <% if (specialEvent.specialEventSwitch === 'ON' && weeklyMarket.weeklyMarketSwitch === 'ON') { %>
            <%- include('two-event-forms', {}); %>
        <% } %>

        <% if (specialEvent.specialEventSwitch !== 'ON' && weeklyMarket.weeklyMarketSwitch === 'ON') { %>
            <%- include('single-weekly-market-form', {}); %>
        <% } %>

        <% if (specialEvent.specialEventSwitch === 'ON' && weeklyMarket.weeklyMarketSwitch !== 'ON') { %>
            <%- include('single-special-event-form', {}); %>
        <% } %>

    </div>
</section>
