<!-- Bring Your Stuff Section -->
<section class="py-5" id="what-is-available">
    <div class="container">
        <h1 class="text-center text-dark mt-0"><%- whatsAvailable.whatsAvailableTitle %></h1>
        <hr class="divider my-4">

        <div class="row">
            <div class="col-6">
                <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block rounded-corner" src="img/produce 1.jpg" alt="First slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block rounded-corner" src="img/produce 2.jpg" alt="Second slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block rounded-corner" src="img/produce 3.jpg" alt="Third slide">
                        </div>
                        <div class="carousel-item">
                            <img class="d-block rounded-corner" src="img/produce 4.jpg" alt="Fourth slide">
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-6 d-inline-block m-auto text-left">
                <h4 class="mb-5"><%- whatsAvailable.item1.title %>&nbsp;<i class="fas fa-carrot "></i></h4>
                <h5 class="mb-3">&#9654; <%- whatsAvailable.item1.subtext.subText1 %></h5>
