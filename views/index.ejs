</header>

<!-- About Section -->
<section class="page-section bg-primary" id="about">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8 text-center">
                <h2 class="text-white mt-0">Who Are We?</h2>
                <hr class="divider light my-4">
                <p class="text-white-50 mb-4">
                    <%- aboutUsBodyStarting %>
                    <% for (var i = 0; i < charitiesAndLink.length - 1;  i++ ) { %>
                        <a class="about-us-links"
                           href="<%= charitiesAndLink[i].charityLink %>"> <%= charitiesAndLink[i].charityName %></a><%= charitiesAndLink[i].additionText %>
                        ,&nbsp;
                    <% } %>
                    <!-- Print out the last charity-->
                    and <a class="about-us-links"
                           href="<%= charitiesAndLink[charitiesAndLink.length - 1].charityLink %>"> <%= charitiesAndLink[charitiesAndLink.length - 1].charityName %></a> <%= charitiesAndLink[i].additionText %>
                    <%= aboutUsBodyEnding %>
                </p>
                <a class="btn btn-light btn-xl js-scroll-trigger" href="#bring-your-stuff">Get Started!</a>
            </div>
        </div>
    </div>
</section>

<!-- Services Section -->
